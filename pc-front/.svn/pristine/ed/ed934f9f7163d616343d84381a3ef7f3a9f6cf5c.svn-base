<div class="zx_list">
    <ul class="i_advisory i_productadvisory i_inventory i_payment i_after i_otherissues">
        <!--商城咨询0626信息开始-->

    </ul>
</div>
<script class="h_advisory h_productadvisory h_inventory h_payment h_after h_otherissues" type="text/x-handlebars-template">
{{#each data}}
<li>
    <p class="hy_xinxi">
        <span>会员：{{accountHan account}}</span>
        <span>会员级别：{{level levelId}}</span>
        <span>咨询时间：{{postTime}}</span>
        <span style={{display replyContent}}>回复时间：{{replyTime}}</span>
    </p>
    <p class="zx_nr">{{content}}</p>
    <p class="zx_hy" style={{display replyContent}}>
        <span class="sp_ico"></span> 商城回复：
        <span style="font-family: 宋体">{{replyContent}}</span>
    </p>
    <p></p>
</li>
{{/each}}
<p class="c"></p>
<!--商城咨询0626信息结束-->
<!--查看全部咨询及分页开始-->
<!-- 分页开始 -->
<div class="allpc">
    <div class="page">
     {{#each sizeArr}}
        <a href="javascript:void(0);" data-module={{module}} data-startRow={{startRow}} data-pageSize={{pageSize}} class={{selected startRow}}>{{size}}</a>
    {{/each}}
            <a style="{{sizeDis}}" target="_blank" href="{{serverUrl}}/goods_zx.html?goodsId={{goodsId}}">查看更多</a>
    </div>
</div>
<!-- 分页结束 -->
<!--查看全部咨询及分页结束-->
<p class="clear"></p>
    </script>