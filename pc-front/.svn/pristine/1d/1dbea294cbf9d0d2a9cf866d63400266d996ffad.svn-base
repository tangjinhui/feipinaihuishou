<ul id="i_record" class="ul1 i_record" style="clear: both;">

    <!-- 分页结束 -->
</ul>
    <script class="h_record" type="text/x-handlebars-template">
        <li class="clearfix first" style="z-index:11;">
            <span class="span1">会员昵称</span>
            <span class="span2">城市</span>
            <span class="span3">订单号</span>
            <span class="span4">数量</span>
            <span class="span5">购买时间</span>
            <span class="span6" style="width: 120px; display: inline-block;">订单状态</span>
        </li>
        {{#each data}}
            <li class="clearfix {{status handle_status}}" style="z-index:{{hqq}};">
                <span class="span1">{{account}}</span>
                <span class="span2">{{address}}</span>
                <span class="span3">{{order_id}}</span>
                <span class="span4">{{quantity}}</span>
                <span class="span5">{{access_date}}</span>
                <span class="span6" style="width: 120px; display: inline-block;">
                    {{status handle_status 1}}
                    <i class="ico_i" style="{{dis order_type}}">
                        <img class="js_move_pic" src="http://static.ecgci.com/images/ico_i.png" alt="">
                        <i class="border_red">{{handle_note_c}} 
                            <i class="border_san">
                                <img src="http://static.ecgci.com/images/san.png" alt="">
                            </i>
                        </i>
                    </i>
                </span>
            </li>
        {{/each}}
    <!-- 分页样式begin -->
    <!-- 分页开始 -->
    <div class="pagetycp">
        <div class="page">
         {{#each sizeArr}}
            <a href="javascript:void(0);" data-module={{module}} data-startRow={{startRow}} data-pageSize={{pageSize}} class={{selected startRow}}>{{size}}</a>
        {{/each}}
        <a style="{{sizeDis}}" target="_blank" href="{{serverUrl}}/buy_record.html?goodsId={{goodsId}}">查看更多</a>
        </div>
    </div>
    </script>