a:hover{
    text-decoration:none;
}
.w .fr{
    float:left;
    border: 1px solid #c1c1c1;
    position: relative;
    left: -1px;
}
.four-nav1{
    width:218px;
    border: 1px solid #c8c8c9;
    border-bottom: 0;
    overflow: inherit;
}
.four-tit{
	width:220px;
    background: #4F4F4F;
    height: 72px;
    line-height: 72px;
    color:#fff;
    letter-spacing: 2px;
    font-size: 16px;
    font-family: Microsoft YaHei;
}
.four-tit div{
    /*display: inline-block;
    margin: 12px 30px;*/
   display: inline;
    margin: 12px 30px;
}
.four-nav1>ul{
    padding:0 0;
}
.four-nav1>ul>li{
	border-bottom: 1px solid #c8c8c9;
}
.four-nav1>ul>li>a{
    font-size: 16px;
    font-family: microsoft yahei;
    font-weight: bold;
    line-height: 36px;
    padding: 0 44px;
    font-family: Microsoft YaHei;
    color:#4f4f4f;
}
.four-nav1>ul>li.show>a{
    background-color:#eee;
    border-bottom:1px solid #fff; 
}
.four-nav1>ul>li #showimg,
.four-nav1>ul>li.show #hideimg{
    display: none;
}
.four-nav1>ul>li.show #showimg,
.four-nav1>ul>li #hideimg{
    display: inline-block;
}
.four-nav1>ul>li img{
    margin-top: -4px;
    margin-left: 7px;
}
.four-nav1>ul>li>ul>li>a{
     line-height: 27px;
     padding-left: 70px;
     font-family: "宋体";
     letter-spacing: 4px;
     height: 27px;/*兼容IE*/
}
.four-nav1>ul>li>ul>li>a.active{
    background: #E7DCDC;
    color: #e43a3d;
    position: relative;
}
.four-nav1>ul>li>ul>li>a.active span{
    display: block;
    width: 0;
    height: 0;
    border-width: 6px 8px 6px 0;
    border-style: solid;
    border-color: transparent #fff transparent;
    position: absolute;
    top: 8px;
    right: -1px;
    z-index:9;
}
/*右侧在线客服小熊猫*/
.xm_nav,.xm_navt{
	transition: .5s;
}
.xm_nav:hover{
	right: -95px;
}
.xm_nav:hover .xm_navt{
	display: block;
}
