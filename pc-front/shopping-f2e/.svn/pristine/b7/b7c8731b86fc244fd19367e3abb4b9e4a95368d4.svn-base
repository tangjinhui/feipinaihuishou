.c {
    clear: both;
}
.left_a {
    width: 218px;
    border: #ccc 1px solid;
    border-right: 1px solid #dedede;
    border-bottom: #999999 3px solid;
    filter: progid:DXImageTransform.Microsoft.Shadow(color='#c9c9c9', Direction=135, Strength=5);
    background-color: #fff;
    -moz-box-shadow: 2px 2px 5px #c9c9c9;
    -webkit-box-shadow: 2px 2px 5px #c9c9c9;
    box-shadow: 2px 2px 5px #c9c9c9;
}
table {
    border: #e1e1e1 1px solid;
    line-height: 24px;
    border-collapse: collapse;
    color: #000;
    font-size: 12px;
}
h2 {
    height: 28px;
    line-height: 28px;
    color: #58595b;
    border: none;
    padding: 0 0 5px;
    font-size: 14px;
    font-weight: bold;
}
th {
    border: none;
}
h1, h2, h3 {
    font-weight: normal;
}
.m_left h2, .m_left h3 {
    font-size: 14px;
    color: #fff;
    font-weight: bold;
    font-family: "Microsoft YaHei";
    text-align: left;
}
.m_left h2 {
    padding: 0 0 0 13px;
}
.m_left h2 {
    height: 44px;
    line-height: 44px;
    color: #333;
    font-size: 18px;
    font-weight: normal;
}
.m_left h2.hycent {
    background: url(http://static.ecgci.com/images/people_bg.png) 0 0 repeat-x;
    font-size: 0;
}
.m_left h3 {
    height: 35px;
    margin: 0;
    padding: 9px 0 0 29px;
    background: #fff;
    border-bottom: 1px dashed #e1e1e1;
    line-height: 35px;
    color: #58595b;
    font-size: 14px;
}
.m_left ul {
    padding: 5px 0;
}
.m_left li {
    padding: 0 0 0 30px;
    color: #58595b;
    line-height: 24px;
    font-size: 12px;
}
.item-reply .reply-list .reply-meta .p-bfc .tbwid {
    margin: 0 auto;
}
h6 {
    padding: 0 10px;
    background: #f3f3f3;
    font-size: 14px;
    height: 30px;
    line-height: 30px;
    border-bottom: #e1e1e1 1px solid;
    margin: 0;
}
em {
    font-style: normal;
}
td, td {
    padding: 10px 20px;
    border: 1px solid #ccc;
    font-weight: normal;
}
a {
    text-decoration: none;
}
a:hover {
    text-decoration: underline
}
table.formtable tr.#viewAddress1 td {
    vertical-align: top
}
a {
    color: #666666;
}
a:hover {
    color: #e43a3d;
}
