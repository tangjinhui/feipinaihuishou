/* mask */

.ui_dialog {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(102, 102, 102, .5);
    display: none;
}
.ui_widget {
    position: absolute;
    overflow: hidden;
    outline: 0px;
    height: 160px;
    width: 300px;
    background-color: #eee;
    top: 50%;
    left: 50%;
    margin-left: -150px;
    margin-top: -80px;
}
.ui_header {
    background: #e43a3d;
    color: #fff;
    opacity: 1;
    padding: 0.5em 0.3em 0.3em 1em;
    position: relative;
    font-family: 'Microsoft YaHei';
    font-size: 14px;
}
.ui_body {
    color: #58595B;
    padding: 0.5em 1em;
    position: relative;
    height: 130px;
}
.ui_body .msg {
    line-height: 18px;
    padding: 20px 0 20px 20px;
}
.ui-button {
    position: absolute;
    right: 10px;
    bottom: 20px;
    background: #F6F6F6;
    border: 1px solid #e5e5e5;
    margin: 5px;
    padding: 0 7px;
    color: #58595b;
    font-weight: bold;
    line-height: 24px;
}
