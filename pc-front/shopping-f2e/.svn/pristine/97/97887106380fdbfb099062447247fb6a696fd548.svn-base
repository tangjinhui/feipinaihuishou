/*block ui*/

.ui-dialog {
    position: relative;
}

.ui-dialog .ui-dialog-titlebar {
    padding: 0.5em 0.3em 0.3em 1em;
    position: relative;
    font-family: 'Microsoft YaHei';
    font-size: 14px;
}

.ui-dialog .ui-dialog-title {
    float: left;
    margin: 0.1em 0 0.2em;
}

