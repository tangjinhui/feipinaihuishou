}
table {
    border: #e1e1e1 1px solid;
    line-height: 24px;
    border-collapse: collapse
}
.bord {
    border: #e1e1e1 1px solid;
}
h2 {
    height: 28px;
    line-height: 28px;
    color: #58595b;
    border: none;
    padding: 0 0 5px;
    font-size: 14px;
    font-weight: bold;
}
.product {
    border: #e1e1e1 1px solid;
    border-left: none;
}
.product #listBox li {
    float: left;
    width: 309px;
    border-left: #e1e1e1 1px solid;
}
.product dl {
    height: 359px;
    border: #fff 8px solid;
    padding: 18px 9px;
    background: #fff;
    overflow: hidden;
}
.product dt {
    width: 240px;
    height: 240px;
    margin: 10px auto 20px;
}
.product dl dd.tit {
    line-height: 22px;
    font-size: 15px;
    color: #333;
    font-family: "Microsoft YaHei";
    overflow: hidden;
}
.product dl dd.tit a {
    color: #333;
}
.product dl dd.tit a:hover {
    color: #e43a3d;
}
.channel_pros {
    width: 1003px;
    margin-bottom: -10px;
    border: none;
    overflow: hidden;
}
.channel_pros #listBox li {
    width: 333px;
    margin-bottom: 40px;
    border: #e1e1e1 1px solid;
    margin-right: -1px;
}
.product dl dd.tit {
    height: 22px;
    overflow: hidden;
}
.page {
    position: relative;
    color: #58595b;
    font: 14px/27px simsun;
    height: 28px;
    line-height: 28px;
    margin: 10px 0;
    text-align: right;
    width: 100%;
}
