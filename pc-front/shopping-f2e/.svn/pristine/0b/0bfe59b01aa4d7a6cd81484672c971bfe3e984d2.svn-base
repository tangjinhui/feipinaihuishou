} */
.page a:hover, .page a.selected {
    background: #e43a3d;
    border: 1px solid #e43a3d;
    color: #fff;
    text-decoration: none;
}
.page a b {
    font-size: 12px;
    font-weight: normal
}
/* .page span {
    font: 12px/27px simsun;
} */
.page span.tiaozhi {
    margin-left: 10px;
    color: #959595;
}
.page input {
    border: 1px solid #e5e5e5;
    height: 22px;
    line-height: 22px;
    padding: 2px 5px;
    width: 25px;
    margin-left: 10px;
    text-align: center;
}
.page input.btn {
    width: 22px;
    height: 22px;
    background: url(http://static.ecgci.com/images/icon.png) -178px -468px no-repeat;
    border: none;
    cursor: pointer;
}
th {
    border: none;
}
.right_a .ss_tj {
    margin-bottom: 10px;
}
.product dl dd.price {
    color: #e43a3d;
    font-size: 12px;
}
h1, h2, h3 {
    font-weight: normal;
}
.btn {
    padding: 2px 8px;
    display: inline-block;
    border: none;
    font-size: 12px;
    vertical-align: middle;
    text-align: center;
    color: #fff;
    font-family: "Microsoft YaHei";
    line-height: 22px;
    cursor: pointer;
    margin-right: 10px;
}
.crumbs {
    height: 52px;
    line-height: 52px;
    color: #58595b;
    font-family: "Microsoft YaHei";
    overflow: hidden;
}
.crumbs span {
    padding: 0 6px;
}
.m_left h2, .m_left h3 {
    font-size: 14px;
    color: #fff;
    font-weight: bold;
    font-family: "Microsoft YaHei";
    text-align: left;
}
.m_left h2 {
    padding: 0 0 0 13px;
}
.m_left h2 {
    height: 44px;
    background: url(http://static.ecgci.com/images/hy_topbg.png) 0 0 repeat-x;
    line-height: 44px;
    color: #333;
    font-size: 18px;
    font-weight: normal;
}
.m_left h3 {
    height: 35px;
    margin: 0;
    padding: 9px 0 0 29px;
    background: #fff;
    border-bottom: 1px dashed #e1e1e1;
    line-height: 35px;
    color: #58595b;
    font-family: Microsoft YaHei;
    font-size: 14px;
}
.m_left ul {
    padding: 5px 0;
}
.m_left li {
    padding: 0 0 0 30px;
    font-family: Microsoft YaHei;
    color: #58595b;
    line-height: 24px;
    font-size: 12px;
}
.red {
    color: #e43a3d;
}
.item-reply .reply-list .reply-meta .p-bfc .tbwid {
    margin: 0 auto;
}
.ss_left {
    border: #e1e1e1 1px solid;
}
.ss_left h3 {
    padding-left: 35px;
    line-height: 26px;
}
.ss_left ul {
    display: none;
}
.ss_left h3 {
    position: relative;
    cursor: pointer;
}
.ss_left h3 span {
    display: block;
    position: absolute;
    top: 15px;
    left: 10px;
    width: 16px;
    height: 16px;
    background: url(http://static.ecgci.com/images/icon.png) 0 0 no-repeat;
}
.over h3 {
    color: #e43a3d;
}
.over h3 span {
    background-position: -21px 0;
}
.ss_tj {
    font-family: 'SimSun';
}
.ss_right h3 {
    background: #f3f3f3;
    margin: 0;
    height: 40px;
    border-bottom: #e1e1e1 1px solid;
    line-height: 40px;
    padding: 0 10px;
    font-size: 14px;
}
.ss_right h3 a {
    font-size: 12px;
}
.ss_tj ul {
    padding: 5px 0 0 10px;
    width: 978px;
}
.ss_tj li {
    width: 100%;
    float: left;
    line-height: 18px;
    padding: 7px 0 5px;
}
.ss_tj dt {
    width: 100px;
    float: left;
    padding: 0 5px 0 0;
    text-align: right;
    font-weight: bold;
}
.ss_tj dd {
    width: 860px;
    float: left;
    overflow: hidden;
}
.ss_tj dl dd a {
    display: block;
    padding: 0 7px;
    height: 18px;
    text-decoration: none;
    margin: 0 20px 4px 0;
    float: left;
}
.ss_tj dl dd a:link, .ss_tj dl dd a:visited {
    color: #58595b;
    text-decoration: none;
}
.ss_tj dl dd a.cur {
    color: #fff;
    text-decoration: none;
    background: #e43a3d;
}
.ss_tj dl dd a.cur:hover {
    color: #fff;
    text-decoration: none;
    background: #e43a3d;
}
.more {
    position: absolute;
    bottom: -28px;
    height: 27px;
    line-height: 27px;
    background: url(http://static.ecgci.com/images/icon.png) #fff no-repeat;
    cursor: pointer;
    padding: 0 20px 0 5px;
    text-align: center;
    width: auto;
    left: 400px;
}
.up-icon {
    background-position: -62px -74px;
    border-left: 1px solid rgb(229, 229, 229);
    border-right: 1px solid rgb(229, 229, 229);
    border-bottom: 1px solid rgb(229, 229, 229);
}
.down-icon {
    background-position: 82px -73px;
    border-left: 1px solid rgb(229, 229, 229);
    border-right: 1px solid rgb(229, 229, 229);
    border-bottom: 1px solid rgb(229, 229, 229);
}
.marg50 {
    margin-top: 38px;
}
.filter {
    background: #f3f3f3;
    height: 25px;
    overflow: hidden;
    padding: 5px 20px 5px 10px;
}
.filter dl, .filter dt, .filter dd {
    float: left;
    line-height: 26px;
}
.filter .total {
    float: right;
    height: 22px;
    line-height: 22px;
    padding: 2px 15px 2px 0;
}
.filter .pagin {
    float: right;
    height: 22px;
    padding: 2px 0;
}
.filter .order dd {
    height: 24px;
    line-height: 24px;
    margin-right: 5px;
    overflow: hidden;
}
.filter .order a {
    display: block;
    padding: 0 10px;
}
.filter .order b {
    display: none;
    margin-left: 5px;
    font-size: 0;
    height: 8px;
    overflow: hidden;
    width: 8px;
}
.filter .order .down b {
    display: inline-block;
    background: url(http://static.ecgci.com/images/icon.png) -75px 0 no-repeat;
}
.filter .order .up b {
    display: inline-block;
    background: url(http://static.ecgci.com/images/icon.png) -57px 0 no-repeat;
}
.filter .order .curr {
    background: #e43a3d;
    font-weight: bold;
}
.filter .order .curr a:link, .filter .order .curr a:visited {
    color: #fff;
}
.filter .order a, .filter .order a:link, .filter .order a:hover, .filter .order a:visited {
    text-decoration: none
}
.pagin a, .pagin span {
    border: 1px solid #e1e1e1;
    float: left;
    font-family: SimSun;
    font-size: 14px;
    height: 20px;
    line-height: 20px;
    margin-left: 2px;
    overflow: hidden;
    padding: 2px 10px;
}
.pagin a {
    position: relative;
}
.pagin a:hover {
    text-decoration: none;
}
.pagin .text {
    border: 0 none;
    padding: 3px 11px;
}
.pagin a b {
    background-image: url(http://static.ecgci.com/images/icon.png);
    background-repeat: no-repeat;
    display: block;
    height: 9px;
    overflow: hidden;
    position: absolute;
    top: 8px;
    width: 5px;
}
.pagin a.prev b {
    background-position: -112px 0;
    left: 3px;
}
.pagin a.next b {
    background-position: -106px 0;
    right: 3px;
}
.pagin a:hover, .pagin a:active {
    background: #e43a3d;
    color: #fff;
}
.pagin a.next.abled:hover b, .pagin a.next.abled:active b {
    background-position: -94px 0;
    right: 3px;
}
.pagin a.prev.abled:hover b, .pagin a.prev.abled:active b {
    background-position: -100px 0;
    right: 3px;
}
.pagin a.unabled, .pagin a.unabled:hover, .pagin a.unabled:active {
    color: #999;
    cursor: default;
    background: #fff;
}
.ss_list_menu {
