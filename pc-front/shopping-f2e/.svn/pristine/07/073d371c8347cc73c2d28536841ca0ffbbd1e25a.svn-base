@import url('http://static.ecgci.com/css/libs/reset.css');
@import url('http://static.ecgci.com/css/libs/header.css');
.dis {
	display: block
}

.dn {
	display: none!important
}

.fl,.fr {
	display: inline
}

.cl {
	clear: both
}

.tc {
	text-align: center
}

.tl {
	text-align: left
}

.tr {
	text-align: right
}

.fl {
	float: left
}

.fr {
	float: right
}

.posr {
	position: relative;
	zoom: 1
}

.posa {
	position: absolute;
	zoom: 1
}

a {
	text-decoration: none;
	color: #333;
	outline: 0
}

a:hover {
	text-decoration: underline
}

#logo a:hover,#nav ul.cate_left li a,#nav ul.cate_left li h2 a,.nav_mune li a,dl.unit a:hover {
	text-decoration: none
}

dl.unit a:hover {
	color: #c42605
}

.highlight {
	color: #bb9a2a
}

.ui_btn,a.fff {
	color: #fff
}

.pad30 {
	padding: 30px 0 0
}

.clearfix:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	content: " ";
	clear: both;
	height: 0
}

.clearfix {
	zoom: 1
}

.break-word {
	word-wrap: break-word;
	word-break: break-all
}

.nowarp {
	white-space: nowrap
}

.wrap {
	width: 1240px;
	margin: 0 auto;
	position: relative;
	z-index: 1
}


.img_loading {
	background: url(data:image/png;
	base64,) center center no-repeat;width: 100%!important;
	height: 100%!important;
	min-height: 60px!important
}

.f12 {
	font-size: 12px
}

.f14 {
	font-size: 14px
}

.f16 {
	font-size: 16px
}

.f18 {
	font-size: 18px
}

.f20 {
	font-size: 20px
}

.imp_p,.imp_r {
	color: #e54346
}

.disable,.disable:hover {
	background-color: #ccc!important
}

.go,.time .bg {
	text-align: center
}

.crumbs.el {
	margin-top: 10px;
	border-left: 12px solid #353535;
	padding-left: 10px;
	height: 19px;
	margin-bottom: 12px;
	width: 1218px;
	border-bottom: 1px solid #353535;
	font-size: 16px;
	color: #353535;
	font-family: "宋体";
	font-weight: 700
}

.go {
	font-size: 22px;
	width: 150px;
	line-height: 45px;
	height: 45px;
	background-color: #b61d1d;
	color: #fff
}

.bg_g {
	background-color: #008d4c
}

.crumbs.el a {
	color: #353535
}

.active_list a:hover,.history a:hover,.will_start a:hover {
	text-decoration: none;
	cursor: pointer
}

.active_list a {
	width: 1240px;
	overflow: hidden;
	display: block;
	float: left
}

.active_list a:hover {
	box-shadow: 0 4px 5px rgba(0,0,0,.4)
}

.active_list dl {
	width: 100%;
	height: 400px;
	margin-bottom: 12px;
	background: #e3bd85
}

.active_list dt,.active_list dt img {
	width: 840px;
	height: 400px
}

.active_list dt {
	overflow: hidden
}

.active_list dd {
	padding: 0 0 0 35px;
	height: 400px
}

.active_list dd .go {
	margin-top: 55px
}

.active_list dd h2 {
	width: 295px;
	line-height: 32px;
	height: 64px;
	margin-top: 25px
}

.active_list dd .now {
	padding-top: 30px
}

.now .ico_ing {
	position: relative;
	background-color: #b61d1d;
	color: #fff;
	width: 55px;
	height: 55px;
	font-size: 18px;
	margin-right: 18px
}

.now .ico_ing .n1,.now .ico_ing .n2,.now .ico_ing .n3,.now .ico_ing .n4 {
	position: absolute
}

.now .ico_ing .n1 {
	left: 8px;
	top: 5px
}

.now .ico_ing .n2 {
	right: 8px;
	top: 5px
}

.now .ico_ing .n3 {
	left: 8px;
	bottom: 5px
}

.now .ico_ing .n4 {
	right: 8px;
	bottom: 5px
}

.now .text {
	font-size: 14px
}

.now .text .t1 {
	max-width: 280px;
	white-space: nowrap;
	margin-top: -3px
}

.now .text .t2 {
	margin-top: 7px
}

.now .text strong {
	font-size: 24px
}

.time {
	font-size: 18px;
	padding-top: 50px
}

.time .left {
	margin-left: -4px
}

.time .bg {
	background-color: #fff;
	width: 29px;
	height: 29px;
	line-height: 29px;
	color: #000;
	margin: 0 3px;
	position: relative;
	top: -2px;
	border-bottom: 1px solid #7f7f7f
}

.time .bg.mm {
	background: url(http://img2.ecgci.com/upload/topic/jingpai_images/jif-2.gif) center center no-repeat #fff
}

.will_start .box {
	width: 1260px
}

.will_start .box .item,.will_start .box .item img {
	width: 610px;
	height: 350px
}

.will_start .box .item {
	border: 1px solid #666;
	float: left;
	overflow: hidden;
	margin-bottom: 15px;
	margin-right: 16px;
	position: relative
}

.box .item .dialog {
	width: 100%;
	height: 100%;
	left: 0;
	top: 0
}

.box .item .dialog .black {
	width: 100%;
	height: 100%;
	position: absolute;
	z-index: 2;
	left: 0;
	top: 0;
	background-color: #1b1b1b;
	filter: alpha(opacity:50);
	opacity: .5
}

.box .item .dialog .small_black {
	width: 338px;
	left: 50%;
	top: 50%;
	position: absolute;
	z-index: 3
}

.box .item .dialog .small_black .black {
	position: absolute;
	z-index: 1;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	background-color: #535353;
	filter: alpha(opacity:90);
	opacity: .9
}

.box .item .dialog .small_black .content {
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	position: absolute;
	z-index: 2;
	color: #fff
}

.small_black .content .padding {
	padding: 20px 0 25px 45px
}

.small_black .content .padding .p1 {
	width: 250px
}

.small_black .content .padding span.bg {
	display: inline-block;
	vertical-align: top;
	margin-top: 2px;
	margin-left: 2px;
	margin-top: 2px\9;
	width: 22px;
	height: 22px;
	background: url(http://img2.ecgci.com/upload/topic/jingpai_images/jif.gif) center center no-repeat
}

.small_black .content p {
	line-height: 26px
}

.small_black .content .go {
	margin-top: 20px
}

.history .item {
	width: 412px;
	border: 1px solid #959595;
	border-right: 0;
	float: left;
	margin-bottom: 14px
}

.history .item.bor {
	border-right: 1px solid #959595
}

.history .item .pic {
	width: 100%;
	height: 300px;
	overflow: hidden;
	position: relative
}

.history .item .pic img {
	width: 100%;
	height: auto;
	min-height: 100%
}

.history .item .pic .ok {
	width: 68px;
	height: 68px;
	background: url(http://img2.ecgci.com/upload/topic/jingpai_images/down.png);
	position: absolute;
	right: 44px;
	top: 27px;
	display: none
}

.history .item.isOk .pic .ok {
	display: block
}

.history .item .tit {
	height: 68px;
	background-color: #f5f5f5;
	overflow: hidden
}

.history .item .tit a {
	display: block;
	width: 86%;
	text-align: left;
	margin: 12px 0 5px 26px;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	font-size: 16px;
}

.history .item .tit div {
	padding: 0 15px 10px 26px;
	line-height: 20px;
	font-size: 14px;
	font-family: '宋体'
}

.history .item .tit div span span {
	color: #b61d1d
}

.history .item .tit a:hover {
	text-decoration: underline
}