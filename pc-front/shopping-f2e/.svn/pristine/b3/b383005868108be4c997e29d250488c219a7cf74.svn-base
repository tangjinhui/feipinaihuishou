/* 自动补全 */

.ac_results {
    background: #fff;
    border: #e43a3d 1px solid;
    border-top: none;
    padding: 0;
    width: 439px;
    z-index: 9;
}
.ac_results ul {
    width: 100%;
}
.ac_results li {
    position: relative;
    cursor: default;
    display: block;
    font-family: "SimSun";
    font-size: 12px;
    line-height: 28px;
    margin: 0;
    overflow: hidden;
    padding: 0 84px 0 6px;
    height: 28px;
}
.ac_even {
    color: #999;
}
.ac_results li:hover,
.ac_results .ac_over {
    background-color: #e43a3d;
    color: #fff;
}
.ac_odd {
    background-color: #f3f3f3;
    color: #999;
}
.ac_results li span {
    position: absolute;
    right: 10px;
    top: 2px;
    text-align: right;
}

