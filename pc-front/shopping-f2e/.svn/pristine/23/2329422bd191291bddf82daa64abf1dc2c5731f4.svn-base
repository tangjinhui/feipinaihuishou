#open_box>div .open_error a {
    color: #0109c3;
}
#open_box .go_open button {
    padding: 10px 18px;
    margin-left: 16px;
    background: #ddd;
    color: #fff;
}
#open_box .go_open button.btndis {
    background: #e43a3d;
    color: #fff;
}
#open_box .open_error_msg {
    width: 862px;
    height: 124px;
    background: #f1f1f1;
}
#open_box .open_error_msg {
    padding: 28px 24px;
}
#open_box .open_error_msg p {
    margin-bottom: 20px;
    line-height: 18px;
    margin-top: 4px;
}
input[type="radio"] {
    margin-right: 5px;
    margin-left: 4px;
    margin-top: 2px;
    cursor: pointer;
}
.open_msg label {
    cursor: pointer;
}
.open_msg a {
    color: #0109c3;
}
h2, h3 {
    font-weight: normal
}
a, button, input {
    -webkit-tap-highlight-color: rgba(255, 0, 0, 0);
}
input, a, button {
    border: 0;
    background: 0;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
input:-webkit-autofill {
    -webkit-box-shadow: 0 0 0px 1000px white inset;
}
