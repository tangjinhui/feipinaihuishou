@import url('http://static.ecgci.com/css/libs/reset.css');
@import url('http://static.ecgci.com/css/libs/header.css');
@import url('http://static.ecgci.com/css/libs/dialog.css');
@import url('../plugin/menu.css');

input[type="checkbox"] {
    width: 14px;
    margin-right: 5px;
}
input[type="radio"] {
    width: 10px;
    margin-right: 5px;
}
.crumbs {
    height: 52px;
    line-height: 52px;
    color: #58595b;
    font-family: "Microsoft YaHei";
    overflow: hidden;
}
.crumbs span {
    padding: 0 6px;
}
.m_left h2, .m_left h3 {
    font-size: 14px;
    color: #fff;
    font-weight: bold;
    font-family: "Microsoft YaHei";
    text-align: left;
}
.m_left h2 {
    padding: 0 0 0 13px;
}
.m_left h2 {
    height: 44px;
    line-height: 44px;
    color: #333;
    font-size: 18px;
    font-weight: normal;
}
.m_left h2.hycent {
    background: url(http://static.ecgci.com/images/people_bg.png) 0 0 repeat-x;
    font-size: 0;
}
.m_left h3 {
    height: 35px;
    margin: 0;
    padding: 9px 0 0 29px;
    background: #fff;
    border-bottom: 1px dashed #e1e1e1;
    line-height: 35px;
    color: #58595b;
    font-family: Microsoft YaHei;
    font-size: 14px;
}
.m_left ul {
    padding: 5px 0;
}
.m_left li {
    padding: 0 0 0 30px;
    font-family: Microsoft YaHei;
    color: #58595b;
    line-height: 24px;
    font-size: 12px;
}
.item-reply .reply-list .reply-meta .p-bfc .tbwid {
    margin: 0 auto;
}
td {
    padding: 10px 20px;
    border: 1px solid #ccc;
    font-weight: normal;
}
table.formtable tr.#viewAddress1 td {
    vertical-align: top
}
input, label {
    vertical-align: middle;
}
button {
    -webkit-appearance: none;
}
.w {
    width: 1240px;
    margin: 0 auto;
}
.left_a {
    width: 218px;
    border: #ccc 1px solid;
    border-right: 1px solid #dedede;
    border-bottom: #999999 3px solid;
    filter: progid:DXImageTransform.Microsoft.Shadow(color='#c9c9c9', Direction=135, Strength=5);
    background-color: #fff;
    -moz-box-shadow: 2px 2px 5px #c9c9c9;
    -webkit-box-shadow: 2px 2px 5px #c9c9c9;
    box-shadow: 2px 2px 5px #c9c9c9;
}
.text_l {
    text-align: left;
}
table, table td {
    border: #e1e1e1 1px solid;
    line-height: 24px;
    border-collapse: collapse
}
table td {
    padding: 5px 10px;
}
h2 {
    height: 28px;
    line-height: 28px;
    color: #58595b;
    border: none;
    padding: 0 0 5px;
    font-size: 14px;
    font-weight: bold;
}
h2, h3 {
    font-weight: normal;
}
#open_box {
    padding: 34px 32px;
}
#open_box>div {
    margin-bottom: 16px;
}
#open_box>div>div {
    float: left;
}
#open_box>div .open_title {
    width: 84px;
    text-align: right;
    margin-right: 32px;
    padding: 10px 0;
}
#open_box>div .open_checkbox {
    width: 114px;
    text-align: right;
    margin-right: 0px;
    padding: 12px 0px 8px 0px;
}
#open_box>div .open_msg {
    width: 260px;
    text-align: left;
    padding: 10px 12px;
    position: relative;
}
#open_box>div .open_read {
    width: 340px;
}
#open_box>div .bor {
    border: 1px solid #a0a0a0;
}
#open_box>div .open_error {
    width: 172px;
    padding: 10px 0;
    margin-left: 44px;
}

/**#open_box>div .open_tip {
    width: 304px;
    padding: 10px 0;
    margin-left: 44px;
}
**/

#open_box .no_border input {
    width: 100%;
    outline: none;
}
#open_box .open_title i {
    color: #e64346;
    margin-right: 4px;
    font-style: normal;
    position: relative;
    top: 2px;
}

