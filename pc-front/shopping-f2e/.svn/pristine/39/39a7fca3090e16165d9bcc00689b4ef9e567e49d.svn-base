.num_change {
    width: 84px;
    height: 26px;
    line-height: 26px;
    color: #777;
    border: 1px solid #777;
    font-size: 14px;
}

.num_change input {
    border: 0 none;
    outline: none;
    height: 26px;
    line-height: 26px;
}

.num_change .num_less, .num_change .num_more {
    width: 26px;
    height: 26px;
    cursor: pointer;
}

.num_change .num_less {
    border-right: 1px solid #777;
}

.num_change .num {
    text-align: center;
    width: 30px;
}

.num_change .num_more {
    border-left: 1px solid #777;
}


