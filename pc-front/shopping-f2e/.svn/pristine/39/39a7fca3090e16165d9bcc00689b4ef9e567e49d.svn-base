.info_wrap {
    margin-top: 20px;
    padding: 30px 10px;
    border: 1px solid #ccc;
}

.info_wrap .line {
    line-height: 25px;
    margin-bottom: 25px;
    width: 100%;
}

.info_wrap .line textarea {
    resize: none;
    width: 400px;
    height: 180px;
}

.info_wrap .img_btn {
    width: 198px;
    height: 38px;
    border: 0 none;
    cursor: pointer;
    background-color: #e85a5d;
    margin-bottom: 10px;
    color: #FFF;
}


