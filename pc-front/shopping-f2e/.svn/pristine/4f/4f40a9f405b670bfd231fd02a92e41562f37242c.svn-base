#showCancerReason {
    margin: 0 auto;
    cursor: default;
    overflow-y: auto;
    overflow-x: hidden;
    width: 461px;
    height: 746px;
    position: fixed;
    top: 30%;
    left: 50%;
    margin-left: -230px;
    z-index: 101;
}

.orderLayer {
    background: #000;
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 100;
    opacity: 0.6;
    filter: alpha(opacity=60);
    top: 0;
    left: 0;
}

.cancelOrderBox {
    display: none;
}
