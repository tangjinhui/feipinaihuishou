<script id="list-template" type="text/x-handlebars-template">
    {{#each list}}
    <tr>
        <td align="center">
            {{id}}
        </td>
        <td align="center">
            {{orderId}}
        </td>
        <td class="text_l">
            <div class="cg_titp">
                <table width="100%" border="0" cellspacing="0" cellpadding="0" style="border: 0 none;" class="fz_12">
                    <tbody>
                        {{#each goods}}
                        <tr>
                            <td valign="middle" style="padding:0;border:none; text-align:left;">
                                {{goodsName}}
                            </td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </td>
        <td align="center">
            {{serviceType}}
        </td>
        <td align="center">
            {{createTime}}
        </td>
        <td align="center">
            {{orderStatus}}
        </td>
        <td align="center">
            <div class="hy_ck">
                {{{show can cancel delivery update detail id orderId}}}
            </div>
        </td>
    </tr>
    {{/each}}
</script>
