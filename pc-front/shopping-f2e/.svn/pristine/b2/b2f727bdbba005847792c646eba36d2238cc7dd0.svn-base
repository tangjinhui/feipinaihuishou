.product {
    border: #e1e1e1 1px solid;
    border-left: none;
}
.product li {
    float: left;
    width: 309px;
    border-left: #e1e1e1 1px solid;
}
.product dl {
    height: 359px;
    border: #fff 8px solid;
    padding: 18px 9px;
    background: #fff;
    overflow: hidden;
}
.product dt {
    width: 240px;
    height: 240px;
    margin: 10px auto 20px;
}
.product dl dd.tit {
    line-height: 22px;
    font-size: 15px;
    color: #333;
    font-family: "Microsoft YaHei";
    overflow: hidden;
}
.product dl dd.tit a {
    color: #333;
}
.product dl dd.tit a:hover {
    color: #e43a3d;
}
.product dl dd.info {
    height: 40px;
    font-size: 12px;
    line-height: 20px;
    color: #808080;
    overflow: hidden;
}
.product .active dl {
    border: #f3f3f3 8px solid;
}
.channel_prov {
    width: 1241px;
    margin-bottom: -40px;
    border: none;
    overflow: hidden;
}
